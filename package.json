{
  "name": "nuxt-monaco-editor",
  "description": "Integrate monaco-editor with Nuxt (3)",
  "keywords": [
    "monaco-editor",
    "nuxt"
  ],
  "author": "e-chan1007",
  "homepage": "https://e-chan1007.github.io/nuxt-monaco-editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/e-chan1007/nuxt-monaco-editor.git"
  },
  "version": "1.2.7",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./dist/*": {
      "import": "./dist/*"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "nuxt-module-build",
    "postpack": "pinst --enable",
    "_postinstall": "husky install && playwright install",
    "build": "nuxt-module-build",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts,.vue . && remark . -q",
    "lint:fix": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts,.vue --fix . && remark . -q",
    "test": "nuxt test",
    "prepack": "pinst --disable && nuxt-module-build",
    "release": "standard-version",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxt prepare playground",
    "docs:dev": "nuxt dev docs",
    "docs:generate": "nuxt generate docs",
    "docs:prepare": "nuxt prepare docs",
    "docs:preview": "nuxt preview docs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.3",
    "defu": "^6.1.4",
    "monaco-editor-nls": "^3.1.0",
    "vite-plugin-static-copy": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@nuxt-themes/docus": "^1.15.0",
    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils": "^3.10.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "eslint": "^8.56.0",
    "execa": "^8.0.1",
    "husky": "^8.0.3",
    "monaco-editor": "^0.45.0",
    "nuxt": "^3.9.3",
    "pinst": "^3.0.0",
    "playwright": "^1.41.0",
    "remark-cli": "^12.0.0",
    "remark-lint": "^9.1.2",
    "remark-preset-lint-recommended": "^6.1.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1",
    "vue": "^3.4.14"
  },
  "peerDependencies": {
    "monaco-editor": "*"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "mixed"
      ]
    ]
  },
  "workspaces": [
    "playground"
  ],
  "packageManager": "yarn@4.0.1"
}